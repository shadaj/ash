# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Assets

GET        /assets/*file        controllers.Assets.at(path="/public", file)

# Home page
GET        /                    me.shadaj.ash.controllers.Application.index
GET        /fast                me.shadaj.ash.controllers.Application.indexFast

GET        /socket              me.shadaj.ash.controllers.Application.socket()

GET        /img                 me.shadaj.ash.controllers.Application.imageProxy(url)